        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/allerta_stencil"
        android:text="Magazine:"
        android:textColor="#FA6700"
        app:layout_constraintStart_toStartOf="parent"
